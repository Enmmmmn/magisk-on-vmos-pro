on post-fs-data
    start logd
    start magisk_daemon

service magisk_daemon /sbin/magisk --post-fs-data
    user root
    seclabel u:r:magisk:s0
    oneshot

service magisk_service /sbin/magisk --service
    class late_start
    user root
    seclabel u:r:magisk:s0
    oneshot

on property:sys.boot_completed=1
    start magisk_boot

service magisk_boot /sbin/magisk --boot-complete
    user root
    seclabel u:r:magisk:s0
    oneshot